void AIThinkHighTemplarSCBW (int player, unit aiUnit, unitgroup scanGroup) {
    marker mark;
    order ord;
    bool lowVitality;
    unitfilter f;
    region r;
    unitgroup enemyGroup;


    if (!AIIsCampaign(player) || AIPlayerDifficulty(player) >= c_campAdvanced) {
